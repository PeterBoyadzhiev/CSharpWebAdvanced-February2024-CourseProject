@model IEnumerable<BusinessAllViewModel>

@{
    ViewData["Title"] = "All Businesses for Approval";
}

<h1 class="text-center">@ViewData["Title"]</h1>
<hr />

@if (Model.Count() == 0)
{
    <h3 class="text-center">No businesses to show.</h3>
}
else
{
    <div class="row mt-3">
        @foreach (var business in Model)
        {
            <partial name="_AdminBusinessPartial" model="@business" />
        }
    </div>
}
